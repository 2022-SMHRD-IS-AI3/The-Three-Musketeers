<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smhrd.mapper.boardMapper">
	<insert id="upload" parameterType="boardDTO">
		insert into web_board values(board_num.nextval, #{board_title}, #{board_content}, #{board_photo}, sysdate, #{category}, #{id})
	</insert>
	<select id="selectAll_board" resultType="boardDTO">
   		select * from board_posts
   	</select>
</mapper>